{
    "name": "webignition/http-history-container",
    "description": "A container for Guzzle history middleware",
    "keywords": ["guzzle", "guzzlehttp", "history", "container", "middleware", "psr-7"],
    "homepage": "https://github.com/webignition/http-history-container",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\HttpHistoryContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\HttpHistoryContainer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-ecs": "./vendor/bin/ecs check src tests --ansi --no-progress-bar --config ecs.php",
        "cs-phpcs": "./vendor/bin/phpcs",
        "cs": [
            "@cs-ecs",
            "@cs-phpcs"
        ],
        "cs-fix": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.php --fix"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@cs",
            "@static-analysis",
            "@test"
        ]
    },
    "require": {
      "php": ">=7.4.0",
      "ext-json": "*",
      "psr/http-message": "^1",
      "psr/log": "^1.1",
      "guzzlehttp/psr7": "^1.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "monolog/monolog": "^2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.78",
        "phpstan/phpstan-mockery": "^0.12.12",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5",
        "symplify/easy-coding-standard": "^8.3",
        "webignition/object-reflector": "^0.2.0"
    },
    "minimum-stability":"stable",
    "config": {
        "sort-packages": true
    }
}
